{
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}